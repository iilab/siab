                "field_hog_what_you_get": {
                    "en": [
                        {
                            "format": "filtered_html", 
                            "safe_value": "<p>The ability to <strong>reduce</strong> the amount of your personal information made public when you use social networking platforms</p>\n", 
                            "value": "The ability to **reduce** the amount of your personal information made public when you use social networking platforms"
                        }, 
                        {
                            "format": "filtered_html", 
                            "safe_value": "<p>The ability to <strong>control</strong> who can access your profile, status updates, photos, and other data on social networking platforms</p>\n", 
